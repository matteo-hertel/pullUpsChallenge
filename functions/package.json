{
  "name": "functions",
  "description": "Cloud Functions for Firebase",
  "engines": { "node": "8" },
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "eslint . --ext js,jsx --fix",
    "prettier": "prettier --config .prettierrc --write \"**/*.{js,jsx}\"",
    "serve": "NODE_ENV=development firebase serve --only functions",
    "shell": "NODE_ENV=development firebase functions:shell",
    "start": "NODE_ENV=development npm run shell",
    "deploy": "firebase deploy --only functions",
    "logs": "firebase functions:log",
    "test": "jest"
  },
  "dependencies": {
    "dayjs": "^1.7.7",
    "eslint": "^5.7.0",
    "eslint-plugin-promise": "^4.0.1",
    "firebase-admin": "^6.0.0",
    "firebase-functions": "^2.0.5",
    "jest": "^26.2.2",
    "lodash": "^4.17.11",
    "moment": "^2.22.2",
    "todoist-js": "^0.3.1",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "eslint-config-skyscanner-with-prettier": "^0.4.0",
    "mockdate": "^2.0.2"
  },
  "private": true
}
